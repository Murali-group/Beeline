FROM continuumio/anaconda3:2018.12

LABEL Maintainer="Aditya Pratapa <adyprat@vt.edu>"

USER root

RUN git clone https://github.com/neocaleb/TENET.git

RUN apt-get update

RUN apt-get install python3 -y

RUN apt-get install python-mpi4py -y

RUN apt-get -y install python3-pip

RUN apt-get install default-jre -y

RUN pip install JPype1

#RUN apt-get install

RUN apt-get install time -y
